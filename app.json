{
  "expo":{
    "name":"Couple Habits",
    "slug":"couple-habits",
    "version":"1.1.5",
    "orientation":"portrait",
    "icon":"./assets/images/icon.png",
    "scheme":"myapp",
    "platforms":[
      "ios",
      "android"
    ],
    "userInterfaceStyle":"automatic",
    "splash":{
      "image":"./assets/images/splash.png",
      "resizeMode":"contain",
      "backgroundColor":"#ffffff"
    },
    "ios":{
      "supportsTablet":false,
      "bundleIdentifier":"com.anonymous.coupleHabits",
      "infoPlist":{
        "NSPhotoLibraryUsageDescription":"Let’s bring your avatar to life! Allow access to your photo library to upload the perfect picture."
      }
    },
    "android":{
      "adaptiveIcon":{
        "foregroundImage":"./assets/images/adaptive-icon.png",
        "backgroundColor":"#ffffff"
      },
      "package":"com.anonymous.couplehabits"
    },
    "web":{
      "bundler":"metro",
      "output":"static",
      "favicon":"./assets/images/favicon.png"
    },
    "plugins":[
      [
        "expo-image-picker",
        {
          "photosPermission":"Let’s bring your avatar to life! Allow access to your photo library to upload the perfect picture."
        }
      ],
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android":{
            "minSdkVersion":24
          }
        }
      ]
    ],
    "experiments":{
      "typedRoutes":true
    },
    "extra":{
      "router":{
        "origin":false
      },
      "eas":{
        "projectId":"2e6a9c12-7a9a-49cd-bb06-53830a8332bb"
      }
    }
  }
}
