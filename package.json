{
  "name": "couplehabits",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prod": "NODE_ENV=production npx expo start",
    "publish-changes:ios": "npx expo publish",
    "eas:submit:ios": "eas submit -p ios --latest",
    "eas:build:android:production": "eas build --platform android --profile production",
    "eas:build:android:preview": "eas build --platform android --profile preview",
    "eas:build:ios:production": "eas build --platform ios",
    "dev": "npx expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "start": "expo start --dev-client"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react-native/extend-expect"
    ]
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.2.21",
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@react-navigation/native": "^6.0.2",
    "@tanstack/react-query": "^5.51.11",
    "axios": "1.7.4",
    "date-fns": "^3.6.0",
    "expo": "~51.0.39",
    "expo-blur": "~13.0.2",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-font": "^12.0.9",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "^13.0.2",
    "expo-linking": "^6.3.1",
    "expo-router": "~3.5.24",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "immer": "^10.1.1",
    "mongoose": "^8.5.2",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-native": "0.74.5",
    "react-native-bouncy-checkbox": "^3.0.5",
    "react-native-image-crop-picker": "^0.41.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-purchases": "^8.2.7",
    "react-native-purchases-ui": "^8.2.7",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-virtualized-view": "^1.0.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.23.8",
    "expo-build-properties": "~0.12.5",
    "expo-dev-client": "~4.0.29"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "react-native-gesture-handler": "~2.16.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
